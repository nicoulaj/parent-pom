            <inherited>true</inherited>
            <executions>
              <execution>
                <id>sign-artifacts</id>
                <phase>verify</phase>
                <goals>
                  <goal>sign</goal>
                </goals>
                <configuration>
                  <useAgent>true</useAgent>
                </configuration>
              </execution>
            </executions>
          </plugin>
          <plugin>
            <artifactId>maven-source-plugin</artifactId>
            <inherited>true</inherited>
            <executions>
              <execution>
                <id>attach-sources</id>
                <goals>
                  <goal>jar-no-fork</goal>
                </goals>
              </execution>
            </executions>
          </plugin>
          <plugin>
            <artifactId>maven-javadoc-plugin</artifactId>
            <inherited>true</inherited>
            <executions>
              <execution>
                <id>attach-javadocs</id>
                <goals>
                  <goal>jar</goal>
                </goals>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>

    <profile>
      <id>license</id>
      <build>
        <defaultGoal>process-sources</defaultGoal>
        <plugins>
          <plugin>
            <groupId>org.codehaus.mojo</groupId>
            <artifactId>build-helper-maven-plugin</artifactId>
            <executions>
              <execution>
                <phase>process-sources</phase>
                <id>store-current-year-property</id>
                <goals>
                  <goal>timestamp-property</goal>
                </goals>
                <configuration>
                  <locale>en,US</locale>
                  <name>year</name>
                  <pattern>yyyy</pattern>
                </configuration>
              </execution>
            </executions>
          </plugin>
          <plugin>
            <groupId>com.mycila</groupId>
            <artifactId>license-maven-plugin</artifactId>
            <executions>
              <execution>
                <phase>process-sources</phase>
                <id>update-license-headers</id>
                <goals>
                  <goal>format</goal>
                </goals>
                <configuration>
                  <aggregate>true</aggregate>
                  <header>${license.header}</header>
                  <properties>
                    <year>${year}</year>
                  </properties>
                  <excludes>
                    <exclude>**/COPYING</exclude>
                    <exclude>**/LICENSE</exclude>
                    <exclude>**/README</exclude>
                    <exclude>**/INSTALL</exclude>
                    <exclude>**/local/**</exclude>
                    <exclude>**/.mvn/**</exclude>
                    <exclude>**/*.tokens</exclude>
                    <exclude>**/*.txt</exclude>
                    <exclude>**/*.sha1</exclude>
                    <exclude>**/*.sha224</exclude>
                    <exclude>**/*.sha256</exclude>
                    <exclude>**/*.sha384</exclude>
                    <exclude>**/*.sha512</exclude>
                    <exclude>**/*.ripemd128</exclude>
                    <exclude>**/*.ripemd160</exclude>
                    <exclude>**/*.ripemd256</exclude>
                    <exclude>**/*.ripemd320</exclude>
                    <exclude>**/*.md2</exclude>
                    <exclude>**/*.md4</exclude>
                    <exclude>**/*.md5</exclude>
                    <exclude>**/*.cksum</exclude>
                    <exclude>**/*.crc32</exclude>
                    <exclude>**/*.gost3411</exclude>
                    <exclude>**/*.tiger</exclude>
                    <exclude>**/*.whirlpool</exclude>
                  </excludes>
                  <mapping>
                    <java>SLASHSTAR_STYLE</java>
                    <g4>JAVADOC_STYLE</g4>
                  </mapping>
                </configuration>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>

  </profiles>

</project>
